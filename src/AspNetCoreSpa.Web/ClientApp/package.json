{
  "name": "aspnetnetcore",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "aspnetcore",
    "entityframework core",
    "angular",
    "oauth",
    "angular-cli",
    "typescript"
  ],
  "repository": "git+https://github.com/asadsahi/AspNetCoreSpa.git",
  "author": "Asad Sahi <asadazmat@gmail.com>",
  "scripts": {
    "ng": "ng",
    "clean:install": "rimraf node_modules & yarn",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:ssr": "npm run build --project=ssr",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/aspnetcorespa/stats.json"
  },
  "dependencies": {
    "@angular/animations": "8.1.0",
    "@angular/common": "8.1.0",
    "@angular/compiler": "8.1.0",
    "@angular/compiler-cli": "8.1.0",
    "@angular/core": "8.1.0",
    "@angular/forms": "8.1.0",
    "@angular/platform-browser": "8.1.0",
    "@angular/platform-browser-dynamic": "8.1.0",
    "@angular/platform-server": "8.1.0",
    "@angular/router": "8.1.0",
    "@aspnet/signalr": "1.1.4",
    "@aspnet/signalr-protocol-msgpack": "1.1.0",
    "@ng-bootstrap/ng-bootstrap": "4.2.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@swimlane/ngx-datatable": "15.0.2",
    "angular-calendar": "0.27.12",
    "angularx-flatpickr": "6.1.0",
    "aspnet-prerendering": "3.0.1",
    "bootstrap": "4.3.1",
    "date-fns": "1.30.1",
    "flatpickr": "4.6.1",
    "font-awesome": "4.7.0",
    "lodash": "^4.17.15",
    "oidc-client": "1.8.2",
    "preboot": "7.0.0",
    "rxjs": "6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.6",
    "@angular/cli": "8.1.0",
    "@angular/language-service": "8.1.0",
    "@angular/pwa": "0.801.0",
    "@angular/service-worker": "8.1.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jest": "^24.0.15",
    "@types/node": "12.0.12",
    "codelyzer": "^5.1.0",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.4.5",
    "webpack-bundle-analyzer": "3.3.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/__mocks__/image.js",
      "@app/(.*)": "<rootDir>/src/app/$1",
      "assets/(.*)": "<rootDir>/src/assets/$1",
      "environments/(.*)": "<rootDir>/src/environments/$1"
    }
  },
  "license": "MIT"
}
